{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@cartridge/tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./dist",
    "types": ["@emotion/react/types/css-prop"]
  },
  "paths": {
    "@cartridge/ui/components": ["./src/components"]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
